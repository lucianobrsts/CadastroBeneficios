	<queryString language="SQL">
		<![CDATA[SELECT cadastrobeneficios.inscrito.nome AS inome,
	cadastrobeneficios.beneficio.data_inicio,
	cadastrobeneficios.beneficio.dia_semana,
	cadastrobeneficios.beneficio.horario_inicio,
	cadastrobeneficios.atividade.nome AS anome
FROM cadastrobeneficios.beneficio
	INNER JOIN cadastrobeneficios.inscrito ON 
	 cadastrobeneficios.beneficio.inscrito_codigo = cadastrobeneficios.inscrito.codigo 
	INNER JOIN cadastrobeneficios.atividade ON 
	 cadastrobeneficios.beneficio.atividade_codigo = cadastrobeneficios.atividade.codigo 
ORDER BY cadastrobeneficios.atividade.nome ASC, 
cadastrobeneficios.inscrito.nome ASC]]>
	</queryString>
